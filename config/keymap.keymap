&none  &kp F6   &kp F7   &kp F8   &kp F9   &kp F10                    &kp RGUI  &kp RSHFT  &kp RCTRL  &kp RALT  &trans  &none
&none  &kp F11  &kp F12  &kp F13  &kp F14  &kp F15  &none     &none   &trans    &trans     &trans     &trans    &trans  &none
&none  &trans   &trans   &trans   &trans   &trans   &trans    &trans  &trans    &trans     &trans     &trans    &trans  &none
            >;
        };

        layer_5 {
            bindings = <
&none  &none      &none           &none           &none            &none                       &none           &none           &none            &none            &none   &none
&none  &trans     &mkp MB4        &msc SCRL_UP    &mkp MB5         &trans                      &trans          &mkp MB4        &mmv MOVE_UP     &mkp MB5         &trans  &none
&none  &kp LSHFT  &mkp MB3        &mkp MB2        &mkp MB1         &trans                      &msc SCRL_UP    &mmv MOVE_LEFT  &mmv MOVE_DOWN   &mmv MOVE_RIGHT  &trans  &none
&none  &kp LCTRL  &msc SCRL_LEFT  &msc SCRL_DOWN  &msc SCRL_RIGHT  &trans  &none     &none     &msc SCRL_DOWN  &msc SCRL_LEFT  &msc SCRL_RIGHT  &trans           &trans  &none
&none  &trans     &trans          &trans          &trans           &trans  &trans    &mkp MB1  &mkp MB2        &none           &none            &none            &trans  &none
            >;
        };

        layer_6 {
            bindings = <
&none  &none          &none          &none          &none          &none                                 &none          &none          &none          &none          &none           &none
&none  &bootloader    &trans         &trans         &bt BT_CLR     &bt BT_CLR_ALL                        &bootloader    &trans         &trans         &bt BT_CLR     &bt BT_CLR_ALL  &none
&none  &bt BT_SEL 0   &bt BT_SEL 1   &bt BT_SEL 2   &bt BT_SEL 3   &bt BT_SEL 4                          &bt BT_SEL 0   &bt BT_SEL 1   &bt BT_SEL 2   &bt BT_SEL 3   &bt BT_SEL 4    &none
&none  &bt BT_DISC 0  &bt BT_DISC 1  &bt BT_DISC 2  &bt BT_DISC 3  &bt BT_DISC 4   &none     &none       &bt BT_DISC 0  &bt BT_DISC 1  &bt BT_DISC 2  &bt BT_DISC 3  &bt BT_DISC 4   &none
&none  &trans         &trans         &trans         &trans         &trans          &trans    &bt BT_PRV  &bt BT_NXT     &none          &none          &none          &trans          &none
            >;
        };
    };
};
